<div class="well">
	<p class="lead">Enter Todays Workout:</p>
	<%= form_for [@workout_type], html: {class: 'form-inline'} do |f|%>
		<div class="form-group">
			<%= f.label :type %>
			<%= f.number_field :type, class: 'form-control' %>
		</div>
		<div class="form-group">
			<%= f.submit "Create Workout", class: "btn btn-success" %>
		</div>
	<% end %>
</div>